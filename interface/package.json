{
  "name": "@brave/swap-interface",
  "version": "0.13.0",
  "description": "Brave Swap - an open-source swap interface by Brave, focussed on usability and multi-chain support.",
  "type": "module",
  "license": "MPL-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/brave/swap.git"
  },
  "keywords": [
    "ethereum",
    "solana",
    "brave",
    "swap",
    "dex"
  ],
  "author": "Brave Software <dev@brave.com>",
  "homepage": "https://brave.com",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16.4.0"
  },
  "files": [
    "dist",
    "dist/style.css"
  ],
  "main": "./dist/lib.umd.cjs",
  "module": "./dist/lib.js",
  "types": "./dist/lib.d.ts",
  "exports": {
    ".": {
      "import": "./dist/lib.js",
      "require": "./dist/lib.umd.cjs"
    },
    "./style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "tsc && vite build -w",
    "build": "tsc && vite build"
  },
  "dependencies": {
    "@brave/leo": "github:brave/leo",
    "react-virtualized-auto-sizer": "^1.0.7",
    "@types/react-window": "^1.8.5",
    "@types/styled-components": "^5.1.26",
    "async": "^3.2.4",
    "bignumber.js": "^9.1.0",
    "ethereum-blockies": "github:brave/blockies",
    "postcss-js": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-window": "^1.8.8",
    "styled-components": "^5.3.6",
    "svelte2tsx": "^0.5.20"
  },
  "devDependencies": {
    "@types/async": "^3.2.15",
    "@types/node": "18.11.12",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@vitejs/plugin-react": "2.1.0",
    "typescript": "4.9.4",
    "vite": "^3.0.0",
    "vite-plugin-dts": "1.6.6"
  }
}
